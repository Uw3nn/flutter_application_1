class Sum {
  int firts;
  int second;

  Sum(this.first, this.second);

  int call() => first + second;
}

import 'data/sum.dart';

void main(){
  var sum = Sum(10, 10);

  var total = sum();
  print(total);
}

class Sum {
  int first;
  int second;

  Sum(this.first, this.second);

  int call() => firts + second;
}

typedef Total = Sum;
typedef Jumlah = Sum;

import 'dat/sum.dart';

void main(){
  var jumlah = Jumlah(10, 10);
  print(jumlah());

  var total = Total(10, 10);
  print(total());
}

typedef Filter = String Funcation(String);

void sayHello(String name, Filter filter) {
  print("Hello ${filter(name)}");
}

void main() {
  sayHello('Eko', (name) => name.toUpperCase());
}

