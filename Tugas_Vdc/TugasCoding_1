  String name = "Guest";
  String? address;
  final String country = "Indonesia";

  Person(String paramName, String paramNameAddress) {
    name = paramName;
    address = paramNameAddress;

  }

  void sayHello(String paramName) {
    print("Hello $paramName, My Name is $name");

  }

var person = Person("Eko Kurniawan", "Subang");

person.name = "Eko Kurniawan";
person.sayHello("Budi");

class Person {
  String name = "Guest";
  String? address;
  final String country = "Indonesia";

  Person(String name, String address) {
    name = name; // field name tidak berubah
    address = address; // field address tidak berubah
  }

}

class Person {
  String name = "Guest";
  String? address;
  final String country = "Indonesia";

  Person(String name, String address){
    this.name = name;
    this.address = address;
  }

}

class Person {
  String name = "Guest";
  String? address;
  final String country = "Indonesia";
 
  Person(this.name, this.address);
  
  }

  Person(this.name, this.address){

  }

  Person.withName(this.name){

  }

  Person.withAddress(this.address){

  }

  var person = Person.withName("Eko Kurniawan");
  var person2 = Person.withAddress("Subang");
  var person3 = Person("Eko Kurniawan", "Jakarta");

  Person(this.name, this.address){

  }
  Person.withName(String name) : this(name, "");
  
  Person.withAddress(String address) : this("", address);

  Person.withJakarta() : this.withAddress("Jakarta");

