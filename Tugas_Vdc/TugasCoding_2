void sayHello() {
  print('Hello World');
}

void main() {
  sayHello();
  sayHello();
}

void sayHello(String firsName, String LastName) {
  print('Hello $firstName $LastName');
}

void main(){
  sayHello('Sera', 'Valentina');
  sayHello('Maria', 'Puspa');
}

void sayHello(String firstName, [String? LastName]) {
  print('Hello $firsName $LastName');
}

void main() {
  sayHello('Sera');
  sayHello('Sera' 'Valentina');
}

void sayHello(String firsName, [String LastName = '']) {
  print('Hello $firstName $LastName');
}

void main() {
  sayHello('Sera');
  sayHello('Sera', 'Valentina');
}

void sayHello({String? firstName, String? LastName}) {
  print('Hello $firstName $LastName');
}

void main(){
  sayHello(firstName: 'Sera' LastName: 'Valentina');
  sayHello(LastName: 'Puspa', firstName: 'Maria');
  sayHello();
  sayHello(firstName: 'Sera');
  sayHello(LastName: 'Sera');
}

void sayHello({String firstName, String LastName = 'Default'}) {
  print('Hello $firstName $LastName');
}

void main() {
  sayHello(firstName: 'Sera', LastName: 'Valentina');
  sayHello(LastName: 'Puspa', firstName: 'Maria');
  sayHello();
  sayHello(firstName: 'Sera');
  sayHello(LastName: 'Sera');
}

int sum(List<int> numbers) {
  var total = 0;
  for (var value in numbers) {
    total += value;
  }
  retur total;
}

void main() {
  print(sum([10, 10, 10, 10, 10]));
  print(sum([5, 5, 5, 5, 5]));
}

int sum(int first, int second) => first + second;

void main() {
  print(sum(10, 10));
  print(sum(20, 20));
}

