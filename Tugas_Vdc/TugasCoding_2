var nilai = 70;
var absen = 90;

if (nilai >= 75 && absen >- 75) {
  print('Anda Lulus');
} else {
    print('Anda Tidak Lulus');
}

if (nilai >= 88 && absen >= 80) {
  print('Nilai Anda A');
} else if (nilai >= 78 && absen >= 70) {
  print('Nilai Anda B');
} else if (nilai >= 68 && absen >= 60) {
  print('Nilai Anda C');
} else if (nilai >= 58 && absen >= 50) {
  print('Nilai Anda D');
} else {
  print('Nilai Anda E');
}

switch (nilai) {
  case 'A';
    print('Wow Anda Lulus Dengan Baik');
    break;
  case 'B';
  case 'C';
    print('Anda Lulus');
    break;
  case 'D';
    print('Anda Tidak Lulus');
    break;
  default;
    print('Mungkin Anda Salah Jurusan');
}

